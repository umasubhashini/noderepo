    <!-- Slider -->
    <header class="header slider-fade">
        <div class="owl-carousel owl-theme">
            <% sliders.sliderOne.forEach((slide) => { %>
                <div class="item bg-img" data-overlay-dark="2" data-background="<%= slide.image %>">
                    <div class="v-middle caption">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-6 slider-text js-fullheight">
                                    <div class="slider-text-inner">
                                        <div class="desc">
                                            <h4 style="<%= getInlineStylesForText(slide.style) %>"><%= slide.text %></h4>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            <% }); %>
            <% sliders.sliderTwo.forEach((slide) => { %>
                <div class="item bg-img" data-overlay-dark="2" data-background="<%= slide.image %>">
                    <div class="v-middle caption">
